    "target": "es5",
    "lib": ["es6", "dom"],
    "types": ["cypress", "node"],
    "module": "commonjs",
    "moduleResolution": "node",
    "preserveValueImports": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["cypress/**/*.ts"]
}
